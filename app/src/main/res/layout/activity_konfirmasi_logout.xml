<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">

    <FrameLayout
        android:layout_width="302dp"
        android:layout_height="310dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_centerInParent="true"
        android:layout_marginStart="60dp"
        android:layout_marginTop="213dp"
        android:layout_marginEnd="52dp"
        android:layout_marginBottom="230dp"
        android:background="@drawable/rectangle6">

        <View
            android:id="@+id/error"
            android:layout_width="143dp"
            android:layout_height="134dp"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="17dp"
            android:background="@drawable/ic_baseline_error_24" />



        <Button
            android:id="@+id/buttonbatal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="176dp"
            android:layout_marginTop="230dp"
            android:backgroundTint="#2BBA5B"
            android:text="Batal" />

        <Button
            android:id="@+id/buttonlogout"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="44dp"
            android:layout_marginTop="230dp"
            android:backgroundTint="#EC002A"
            android:text="Keluar" />
        <TextView
            android:id="@+id/keluar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Anda yakin ingin keluar?"
            android:layout_marginStart="63dp"
            android:layout_marginTop="180dp"
            android:textSize="14dp" />
    </FrameLayout>


</RelativeLayout>